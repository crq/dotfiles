[user]
	name = Charles Quirin
	email = crq@users.noreply.github.com
[core]
	excludesfile = ~/.gitignore_global
[credential]
	helper = osxkeychain
[push]
	default = simple
[alias]
	co = checkout
	br = branch
	ci = commit
	s = status -s
	d = diff
        l = log --pretty=format:"%C(yellow)%h %ad%Cred%d: %Creset%s%Cblue - %cn" --decorate --date=short
        a = add
        ap = add -p
        la = "!git config -l | grep alias | cut -c 7-"
