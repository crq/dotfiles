	// 	],
	// 	"description": "Log output to console"
	// }
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1')"
		],
		"description": "Log output to console"
	},
	"it block": {
		"prefix": "it",
		"body": [
			"it('$1', () => {",
			"  $0",
			"})"
		],
		"description": "create jest it block"
	},
	"Describe block": {
		"prefix": "desc",
		"body": [
